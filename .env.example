# Example .env file
# Copy this to .env and fill in your actual values

# Email Configuration (for registration/password reset)
GMAIL_USER=your_email@gmail.com
GMAIL_APP_PASSWORD=your_gmail_app_password

# Stripe Configuration (get from https://dashboard.stripe.com/test/apikeys)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Frontend Stripe Configuration (React Native/Expo)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Optional: Stripe Webhook Secret (if using Stripe CLI)
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
